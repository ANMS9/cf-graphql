{
  "name": "cf-graphql",
  "description": "Generate a GraphQL schema out of your Contentful space",
  "version": "0.3.0",
  "license": "MIT",
  "repository": "jelz/cf-graphql",
  "contributors": [
    {
      "name": "Jakub Elżbieciak",
      "email": "jelz@post.pl",
      "url": "https://elzbieciak.pl/"
    },
    {
      "name": "Michael Elsdörfer",
      "email": "michael@elsdoerfer.com",
      "url": "http://blog.elsdoerfer.name"
    }
  ],
  "keywords": [
    "graphql",
    "contentful",
    "graph",
    "schema",
    "cda"
  ],
  "main": "index.js",
  "scripts": {
    "dump": "mkdirp dump && npm run dump-graph && npm run dump-schema",
    "dump-graph": "graphqlviz http://localhost:4000/graphql | dot -Tpng -o dump/graph.png",
    "dump-schema": "fetch-graphql-schema http://localhost:4000/graphql -r -o dump/schema.graphql",
    "lint": "eslint 'index.js' 'src/*.js' 'test/*.js' 'demo/*.js'",
    "test": "tape 'test/*.js'",
    "coverage": "istanbul cover tape 'test/*.js'",
    "codecov": "cat coverage/coverage.json | codecov"
  },
  "dependencies": {
    "dataloader": "^1.3.0",
    "graphql": "^0.9.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.chunk": "^4.2.0",
    "lodash.get": "^4.4.2",
    "lodash.upperfirst": "^4.3.1",
    "node-fetch": "^1.6.3",
    "pluralize": "^4.0.0"
  },
  "devDependencies": {
    "codecov": "^2.1.0",
    "eslint": "^3.19.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "fetch-graphql-schema": "^0.2.1",
    "graphqlviz": "^2.0.1",
    "istanbul": "^0.4.5",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0",
    "tape": "^4.6.3"
  }
}
